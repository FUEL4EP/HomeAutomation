rm -f /www/config/img/devices/50/$DEVICE_THUMB

#------------------------------------------------------------------------------
#------------------------------------------------------------------------------
# device name            : HB-UNI-SenAct-4-4-RC_DS
# small case device name : hb-uni-senact-4-4-RC_DS
# XML version            : not specified
# index                  : 14
#------------------------------------------------------------------------------
#------------------------------------------------------------------------------
# in der WebUI angezeigter Gerätetyp, muss identisch sein mit dem Firmware-XML-Tag: <type name="HB-UNI-SenAct-4-4-RC_DS" id="HB-UNI-SenAct-4-4-RC_DS">
DEVICE="HB-UNI-SenAct-4-4-RC_DS"
# in der WebUI angezeigte Gerätebeschreibung
DEVICE_DESC="3x Netzschalter mit hybridem Akku-\/Netzbetrieb f\&uuml;r Netzfreischalter"
# Name der Piktogramme, bestehend aus xx.png bzw. xx_thumb.png
DEVICE_IMG=hb-uni-senact-4-4-RC_DS.png
DEVICE_THUMB=hb-uni-senact-4-4-RC_DS_thumb.png
FIRMWARE_FILE=/firmware/rftypes/hb-uni-senact-4-4-RC_DS*.xml

#------------------------------------------------------------------------------
# Undo DEVDB.tcl changes
devdescrSearch="$DEVICE {{50 \/config\/img\/devices\/50\/$DEVICE_THUMB} {250 \/config\/img\/devices\/250\/$DEVICE_IMG}} "

if [ "`cat $devdescrFile | grep \"$DEVICE\"`" ]; then
        sed -i "s/\($devdescrSearch\)//g" $devdescrFile
fi

#------------------------------------------------------------------------------
# Undo webui.js changes
if [ "`cat $webuiFile | grep \"$DEVICE\"`" ]; then
        sed -i "/\($DEVICE\)/d" $webuiFile
fi

#------------------------------------------------------------------------------
# remove device_xml links
rm -f $FIRMWARE_FILE

#------------------------------------------------------------------------------
# remove thumb files
rm -f /www/config/img/devices/250/$DEVICE_IMG
rm -f /www/config/img/devices/50/$DEVICE_THUMB

#------------------------------------------------------------------------------
#------------------------------------------------------------------------------
#------------------------------------------------------------------------------
# ab hier siehe Anleitung von Jérôme, siehe https://homematic-forum.de/forum/viewtopic.php?f=76&t=60067
#------------------------------------------------------------------------------
#------------------------------------------------------------------------------
#------------------------------------------------------------------------------


